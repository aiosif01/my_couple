<?xml version="1.0" encoding="UTF-8" ?>
<precice-configuration>
  <log>
    <sink type="stream" output="stdout" filter="%Severity% > debug" format="preCICE:%ColorizedSeverity% %Message%" enabled="true" />
  </log>

  <data:vector name="Force" />
  <data:vector name="Stress" />

  <mesh name="Cavity-Mesh" dimensions="3">
    <use-data name="Force" />
    <use-data name="Stress" />
  </mesh>

  <participant name="Cavity">
    <provide-mesh name="Cavity-Mesh" />
    <write-data name="Force" mesh="Cavity-Mesh" />
    <read-data name="Stress" mesh="Cavity-Mesh" />
  </participant>

  <m2n:sockets acceptor="Cavity" connector="dummy" />

  <coupling-scheme:serial-explicit>
    <participants first="Cavity" second="dummy" />
    <max-time-windows value="100" />
    <time-window-size value="0.005" />
    <exchange data="Force" mesh="Cavity-Mesh" from="Cavity" to="dummy" />
    <exchange data="Stress" mesh="Cavity-Mesh" from="dummy" to="Cavity" />
  </coupling-scheme:serial-explicit>
</precice-configuration>